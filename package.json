{
  "name": "@amplitude/storybook-addon-amplitude",
  "version": "2.1.0",
  "description": "A storybook addon to capture events in Amplitude",
  "keywords": [
    "amplitude",
    "design-systems",
    "storybook-addons"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amplitude/storybook-addon-amplitude.git"
  },
  "type": "module",
  "author": "Amplitude Inc",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/ts/index.d.ts"
    },
    "./manager": "./dist/esm/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "buildBabel": "concurrently \"yarn buildBabel:cjs\" \"yarn buildBabel:esm\"",
    "buildBabel:cjs": "babel ./src -d ./dist/cjs --extensions \".js,.jsx,.ts,.tsx\"",
    "buildBabel:esm": "babel ./src -d ./dist/esm --env-name esm --extensions \".js,.jsx,.ts,.tsx\"",
    "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "prebuild": "yarn clean",
    "build": "concurrently \"yarn buildBabel\" \"yarn buildTsc\"",
    "build:watch": "concurrently \"yarn buildBabel:esm -- --watch\" \"yarn buildTsc -- --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prerelease": "zx scripts/prepublish-checks.mjs",
    "release": "yarn build && auto shipit"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^1.0.0 || ^2.0.0",
    "global": "^4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/dom": "10.4.0",
    "@types/node": "^24.0.3",
    "@types/webpack-env": "^1.18.8",
    "auto": "^11.1.0",
    "babel-loader": "^10.0.0",
    "boxen": "^8.0.1",
    "concurrently": "^9.1.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "storybook": "^9.0.0",
    "typescript": "^5.8.3",
    "webpack": "^5.98.0",
    "zx": "^8.5.5"
  },
  "peerDependencies": {
    "storybook": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Amplitude Storybook Addon",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "engines": {
    "node": "20"
  }
}
